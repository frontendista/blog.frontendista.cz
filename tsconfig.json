{
	"extends": "astro/tsconfigs/strictest",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/styles/*": [
				"./src/styles/*"
			],
			"@/components/*": [
				"./src/components/*"
			],
			"@/paraglide/*": [
				"./src/paraglide/*"
			]
		},
	},
	"include": [
		".astro/types.d.ts",
		"**/*"
	],
	"exclude": [
		"dist"
	]
}